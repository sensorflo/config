[user]
	name = sensorflo
	email = sensorflo@gmail.com
[credential]
  username = sensorflo
	helper = cache --timeout=3600
[branch]
	autosetuprebase = always
[color]
  ui = true
[core]
  editor = emacs -nw -Q
[merge]
  conflictstyle = diff3
  tool = meld
[diff]
  algorithm = patience
[mergetool "meld"]
  # note that this creates _two_ tabs in meld
  cmd = meld --diff $LOCAL $MERGED $REMOTE --diff $LOCAL $BASE $REMOTE
[alias]
  rbi = rebase --interactive
  rba = rebase --abort
  rbc = rebase --continue
  m = merge
  ma = merge --abort	
  mt = mergetool
  cp = cherry-pick
  b = branch
  ba = branch -vva
  c = commit
  f = fetch
  p = push
  co = checkout
  cob = checkout -b
  st = status
[push]
	default = simple
